<h1>APIM Sign Up</h1>
<form action="/signUp" method="post">
    <label for="firstName">First name</label>
    <input id="firstName" name="firstName" type="text"/>
    <label for="lastName">Last name</label>
    <input id="lastName" name="lastName" type="text"/>
    <label for="email">Email</label>
    <input id="email" name="email" type="email"/>
    <label for="password">Password</label>
    <input id="password" name="password" type="password"/>
    <input type="submit" name="signUp" value="Sign up"/>
    <input type="hidden" name="operation" value="<%= signUpSignInRequest.operation %>"/>
    <input type="hidden" name="returnUrl" value="<%= signUpSignInRequest.returnUrl %>"/>
    <input type="hidden" name="salt" value="<%= signUpSignInRequest.salt %>"/>
    <input type="hidden" name="sig" value="<%= signUpSignInRequest.sig %>"/>
</form>
<p><%= errorMessage %> </p>